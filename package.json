{
  "name": "pico-gltest",
  "version": "0.0.5",
  "description": "A test framework for WebGL 1 and 2 applications",
  "bin": {
    "gltest": "gltest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsherif/pico-gltest"
  },
  "scripts": {
    "lint": "eslint gltest page/gltest.js",
    "test": "npm run lint && ./gltest",
    "coverage": "rimraf coverage/ && ./gltest --self-test --coverage && nyc report --reporter=lcov"
  },
  "keywords": [
    "webgl",
    "webgl2",
    "test",
    "puppeteer"
  ],
  "author": {
    "name": "Tarek Sherif",
    "email": "tareksherif@pm.me",
    "url": "http://tareksherif.net/"
  },
  "license": "MIT",
  "dependencies": {
    "copy-dir": "^1.2.0",
    "node-glob": "^1.2.0",
    "puppeteer": "^2.0.0",
    "puppeteer-to-istanbul": "^1.2.2",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.7",
    "eslint": "^6.6.0",
    "nyc": "^14.1.1"
  }
}
